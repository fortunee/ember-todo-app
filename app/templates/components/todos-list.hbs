{{title}}

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Todo ID</th>
      <th>Todo Item</th>
    </tr>
  </thead>
  <tbody>
    {{#each todos as |todo|}}
      <tr>
        <td>{{todo.id}}</td>
        <td>{{todo.todoItem}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>